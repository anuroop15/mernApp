  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0"
