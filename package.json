  "description": "reusable template for any kinda application",
  "main": "src/server.js",
  "scripts": {
    "start": "NODE_ENV=production node_modules/.bin/babel-node --presets react,es2016 src/server.js",
    "test": "npm run test",
    "dev": "./node_modules/.bin/http-server",
    "start": "cross-env NODE_ENV=production node_modules/.bin/babel-node --presets react,es2015 src/server.js",
    "start-dev": "npm run start-dev-hmr",
    "start-dev-single-page": "node_modules/.bin/http-server src/static",
    "start-dev-hmr": "./node_modules/.bin/webpack-dev-server --progress --inline --hot --open",
    "dev-build": "cross-env NODE_ENV=production node_modules/.bin/webpack -p"
  },
  "repository": {
    "type": "git",
