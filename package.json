{
  "name": "anuroop-invitation",
  "version": "1.0.0",
  "description": "reusable template for any kinda application",
  "main": "src/server.js",
  "scripts": {
    "start": "NODE_ENV=development nodemon src/server.js --exec babel-node --presets es2015,stage-2",
    "test": "npm run test",
    "dev": "./node_modules/.bin/http-server",
    "start-dev": "npm run start-dev-hmr",
    "start-dev-single-page": "nodemon ./node_modules/.bin/http-server src/static --presets react,es2015,stage-2",
    "start-dev-hmr": "./node_modules/.bin/webpack-dev-server --progress --inline --hot --open",
    "dev-build": "./node_modules/.bin/webpack --config webpack.config.js --watch --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anuroop15/node-with-react.git"
  },
  "author": "Anuroop-Navya",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anuroop15/node-with-react/issues"
  },
  "homepage": "https://github.com/anuroop15/node-with-react#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "http-server": "^0.10.0",
    "nodemon": "^1.12.5",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  }
}
